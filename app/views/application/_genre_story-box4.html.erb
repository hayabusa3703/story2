<% @jobs.each do |job| %>
  <div class="story-box">
    <span class="name"></span>
    <%= link_to(story_path(job), class: 'wall', style: "background-image: url(https://img.sty.fun/t/uploads/user/wall/5126/cc0ee168.jpeg);") do %><% end %>
  <div class="category">
    <div data-is="print" class="category__label love category__label__after" href="/category/120" label="<%= job.category.name %>" style="background-color: <%= job.category.color %>;"><%= job.category.name %>
    </div>
  </div>
  <%= link_to(story_path(job), class: 'wrapper') do %>
    <div class="top clearfix">
      <div class="left">
        <div class="left img" style="background-image: url(https://graph.facebook.com/727353672/picture?width=400&amp;height=400);"></div>
      </div>
      <div class="right-area">
        <div class="right-user">
          <div class="story-name" data-is="print" label="user_name"><%= job.user.name %></div>
        </div>
        <div class="reading-time">4 min read</div>
        <div class="right-area clearfix"></div>
        <h3 class="topic">
          <span><p><%= job.title %></p></span>
        </h3>
      </div>
    </div>
    <div class="story-discription">
      <div class="etc-reader">
        <p><%= job.text %></p>
      </div>
    </div>
    <div>
      <div data-is="story-box-users" bookmark_counter="22">
        <div class="users-wrapper">
          <div class="users">
            <img src="">
            <img src="">
            <img src="">
            <img src="">
          </div>
          <div class="text">
            <div class="num" data-is="print" label="○○人の"><%= job.likes_count %>人の</div>
            <div class="sub_text">読んでよかった！！</div>
          </div>
        </div>
      </div>
      <div data-is="story-box-comment">
        <div class="users-wrapper clearfix">
          <div class="left">
            <div class="img" style="background-image: url(https://graph.facebook.com/100002151358967/picture?width=400&amp;height=400);" title="内藤 涼子"></div>
          </div>
          <div class="body">
            <div class="inner" data-is="print" label=""></div>
          </div>
        </div>
      </div>
    </div>
  <% end %>
  </div>
<% end %>
